#include<iostream>
using namespace std;
int main(){
    int row, col;
    cout<<"Enter the dimensions of the matrix : ";
    cin>> row >> col;
    int  arr[row][col];
    cout<<"Enter the elements in the matrix : ";
    for(int i=0;i<row;i++){
        for(int j=0;j<col;j++){
            cin>>arr[i][j];
        }
    }
    cout<<"THe Matrix : ";
    for(int i=0;i<row;i++){
        for(int j=0;j<col;j++){
            cout<<arr[i][j]<<" ";
        }
    }
}
