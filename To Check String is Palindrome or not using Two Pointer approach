#include<iostream>
using namespace std;
int main(){
    string str;
    cout<<"Enter the string : ";
    cin>>str;
    int start=0,end=str.length()-1;
    
    while(start<=end){
        str[start] = tolower(str[start]);
        str[end] = tolower(str[end]);
        if(str[start]==str[end]){
            start++;
            end--;
        }
        else{
            cout<<"Given String is not Palindrome"<<endl;
            break;
        }
    }
    if(start>end){
        cout<<"Given String is Palindrome "<<endl;
    }
}
